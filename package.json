{
	"name": "beatrice",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"lint": "next lint",
		"start": "npm run db:push && next start",
		"deps": "node licenses.js > lib/licenses.ts",
		"db:gen": "prisma generate",
		"db:push": "prisma migrate deploy",
		"build": "npm run build:all",
		"build:main": "npm run deps && npm run db:gen && next build",
		"build:all": "npm run build:main && npm run build:files",
		"format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" --config ./.prettierrc --ignore-path ./.gitignore",
		"build:files": "cd files && npx tsc"
	},
	"dependencies": {
		"@next-auth/prisma-adapter": "^1.0.3",
		"@prisma/client": "^3.12.0",
		"dotenv": "^16.0.0",
		"formidable": "^2.1.2",
		"jsonwebtoken": "^9.0.0",
		"mime-types": "^2.1.35",
		"next": "13.5.6",
		"next-auth": "^4.24.5",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"request-ip": "^2.1.3"
	},
	"devDependencies": {
		"@types/formidable": "^2.0.5",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/license-checker": "^25.0.3",
		"@types/mime-types": "^2.1.1",
		"@types/node": "17.0.23",
		"@types/react": "18.0.4",
		"@types/react-dom": "18.0.0",
		"@types/request-ip": "^0.0.37",
		"eslint": "8.13.0",
		"eslint-config-next": "12.1.5",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"license-checker": "^25.0.1",
		"marked": "^4.0.14",
		"prettier": "^2.6.2",
		"prisma": "^3.12.0",
		"typescript": "4.6.3"
	}
}
